{
	"arrowParens": "avoid",
	"semi": true,
	"bracketSameLine": false,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": false,
	"printWidth": 80,
	"proseWrap": "preserve",
	"editorconfig": true,
	"overrides": [
		{
			"files": ["*.md", "*.yml", "*.yaml", "*.toml", "*.rb", "*.md"],
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		}
	]
}
